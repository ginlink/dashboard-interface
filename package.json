    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "i18next": "^15.0.9",
    "i18next-browser-languagedetector": "^6.1.2",
