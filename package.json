    "ethers": "^5.4.6",
    "fortmatic": "^2.2.1",
    "http-proxy-middleware": "^1.0.5",
    "i18next-http-backend": "^1.3.0",
    "jsbi": "^4.1.0",
    "multicodec": "^3.0.1",
    "polished": "^4.1.3",
    "react": "^16.13.1",
    "react-app-rewired": "2.1.8",
    "react-device-detect": "^1.17.0",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.9",
    "react-intl": "^5.20.10",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.2.0",
