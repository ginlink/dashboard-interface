{
  "name": "converter-bsc-web",
  "version": "1.0.0",
  "description": ".",
  "author": ".",
  "private": true,
  "scripts": {
    "start": "yarn compile-contract-types && react-app-rewired start",
    "start:origin": "yarn compile-contract-types && react-scripts start",
    "build": "yarn compile-external-abi-types && yarn build:prod",
    "build:prod": "cross-env NODE_ENV_CUSTOM=production react-app-rewired  build --mode production",
    "build:test": "cross-env NODE_ENV_CUSTOM=testing react-app-rewired build --mode testing",
    "build:pre": "cross-env NODE_ENV_CUSTOM=preview react-app-rewired build --mode preview",
    "build:dev": "cross-env NODE_ENV_CUSTOM=development react-app-rewired build --mode development",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "compile-contract-types": "yarn compile-external-abi-types && yarn compile-v3-contract-types",
    "compile-external-abi-types": "typechain --target ethers-v5 --out-dir src/abis/types './src/abis/**/*.json'",
    "compile-v3-contract-types": "typechain --target ethers-v5 --out-dir src/types/v3 './src/plugins/@uniswap/?(v3-core|v3-periphery)/artifacts/contracts/**/*.json'"
  },
  "dependencies": {
